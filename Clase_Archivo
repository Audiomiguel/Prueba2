#pragma once
#include <string>
#include <iostream>
using namespace std;

class HistoriasClinicas
{
private:
	string nombre;
	long long tamanio;
	string extension;
	string direccion;
	string fecha;
public:
	HistoriasClinicas(string nombre, long long tamanio, string extension,
		string fecha, string direccion)
		: nombre(nombre), tamanio(tamanio), extension(extension),
		direccion(direccion), fecha(fecha) {};
	~HistoriasClinicas() {}

	string getNombre() { return nombre; }
	long long getTamanio() { return tamanio; }
	string getExtension() { return extension; }
	string getDireccion() { return direccion; }
	string getFecha() { return fecha; }

};
